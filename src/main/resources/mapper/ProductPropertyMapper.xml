<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ksl.product.mapper.ProductPropertyMapper">

	<resultMap id="BaseResultMap" type="com.ksl.product.entity.ProductProperty">
		<id column="id" property="id" />
		<result column="property_name" property="propertyName" />
		<result column="deleted" property="deleted" />
		<result column="remark" property="remark" />
		<result column="create_by" property="createBy" />
		<result column="create_time" property="createTime" />
		<result column="update_by" property="updateBy" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, property_name, deleted, remark, create_by, create_time, update_by, update_time
    </sql>
</mapper>
