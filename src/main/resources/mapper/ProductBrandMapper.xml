<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ksl.product.mapper.ProductBrandMapper">

	<resultMap id="BaseResultMap" type="com.ksl.product.entity.ProductBrand">
		<id column="id" property="id" />
		<result column="brand_code" property="brandCode" />
		<result column="brand_name_cn" property="brandNameCn" />
		<result column="brand_name_en" property="brandNameEn" />
		<result column="brand_logo" property="brandLogo" />
		<result column="untitled" property="untitled" />
		<result column="create_by" property="createBy" />
		<result column="create_time" property="createTime" />
		<result column="update_by" property="updateBy" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, brand_code, brand_name_cn, brand_name_en, brand_logo, untitled, create_by, create_time, update_by, update_time
    </sql>
</mapper>
